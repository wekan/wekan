	"unused": true,
	"strict": true,
	"trailing": true,
	"maxparams": false,
	"maxdepth": false,
	"maxstatements": false,
	"maxcomplexity": false,
	"maxlen": 80,
	"asi": false,
	"boss": false,
	"debug": false,
	"eqnull": false,
	"es5": false,
	"esnext": false,
	"moz": false,
	"evil": false,
	"expr": false,
	"funcscope": false,
	"globalstrict": true,
	"iterator": false,
	"lastsemic": false,
	"laxbreak": false,
	"laxcomma": false,
	"loopfunc": false,
	"multistr": false,
	"proto": false,
	"scripturl": false,
	"smarttabs": false,
	"shadow": false,
	"sub": false,
	"supernew": false,
	"validthis": false,
	"browser": true,
